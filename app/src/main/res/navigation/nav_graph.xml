<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment_to_signupFragment"
            app:destination="@id/signupFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_startFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_startFragment_to_patientFragment"
            app:destination="@id/patientFragment"
            app:popUpTo="@id/startFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_startFragment_to_doctorDashboardFragment"
            app:destination="@id/doctorDashboardFragment"
            app:popUpTo="@id/startFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_signupFragment"
            app:destination="@id/signupFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_loginFragment_to_patientFragment"
            app:destination="@id/patientFragment"
            app:popUpTo="@id/startFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_loginFragment_to_doctorDashboardFragment"
            app:destination="@id/doctorDashboardFragment"
            app:popUpTo="@id/startFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

    </fragment>
    <fragment
        android:id="@+id/signupFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.SignupFragment"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_signup" >
        <action
            android:id="@+id/action_signupFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/signupFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_signupFragment_to_doctorInfoFragment"
            app:destination="@id/doctorInfoFragment"
            app:popUpTo="@id/doctorInfoFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_signupFragment_to_patientFragment"
            app:destination="@id/patientFragment"
            app:popUpTo="@id/startFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>

    </fragment>
    <fragment
        android:id="@+id/doctorInfoFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.DoctorInfoFragment"
        android:label="fragment_doctor_info"
        tools:layout="@layout/fragment_doctor_info" >
        <action
            android:id="@+id/action_doctorInfoFragment_to_doctorDashboardFragment"
            app:destination="@id/doctorDashboardFragment"
            app:popUpTo="@id/doctorDashboardFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/patientFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.PatientFragment"
        android:label="PatientFragment" >
        <action
            android:id="@+id/action_patientFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:popUpTo="@id/startFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_patientFragment_to_chooseClinicFragment"
            app:destination="@id/chooseClinicFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_patientFragment_to_bloodDonationFragment"
            app:destination="@id/bloodDonationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_patientFragment_to_bloodTransfusionFragment"
            app:destination="@id/bloodTransfusionFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/doctorDashboardFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.DoctorDashboardFragment"
        android:label="fragment_doctor_dashboard"
        tools:layout="@layout/fragment_doctor_dashboard" >
        <action
            android:id="@+id/action_doctorDashboardFragment_to_myAppointmentsFragment"
            app:destination="@id/myAppointmentsFragment" />
        <action
            android:id="@+id/action_doctorDashboardFragment_to_addAppointmentsFragment"
            app:destination="@id/addAppointmentsFragment" />
        <action
            android:id="@+id/action_doctorDashboardFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:popUpTo="@id/doctorDashboardFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/myAppointmentsFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.MyAppointmentsFragment"
        android:label="fragment_my_appointments"
        tools:layout="@layout/fragment_my_appointments" />
    <fragment
        android:id="@+id/addAppointmentsFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.AddAppointmentsFragment"
        android:label="fragment_add_appointment"
        tools:layout="@layout/fragment_add_appointment" />
    <fragment
        android:id="@+id/chooseClinicFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.ChooseClinicFragment"
        android:label="ChooseClinicFragment"
        app:enterAnim="@anim/slide_in_right"
        app:exitAnim="@anim/slide_out_left"
        app:popEnterAnim="@anim/slide_in_left"
        app:popExitAnim="@anim/slide_out_right">
        <action
            android:id="@+id/action_chooseClinicFragment_to_chooseDoctorFragment"
            app:destination="@id/chooseDoctorFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/chooseDoctorFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.ChooseDoctorFragment"
        android:label="ChooseDoctorFragment" >
        <action
            android:id="@+id/action_chooseDoctorFragment_to_patientFragment"
            app:destination="@id/patientFragment"
            app:popUpTo="@id/patientFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/bloodDonationFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.BloodDonationFragment"
        android:label="BloodDonationFragment" />
    <fragment
        android:id="@+id/bloodTransfusionFragment"
        android:name="com.ali.ehab.healthcare.ui.fragments.BloodTransfusionFragment"
        android:label="fragment_blood_transfusion"
        tools:layout="@layout/fragment_blood_transfusion" />

</navigation>